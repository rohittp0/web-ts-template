  "bugs": {
    "url": "https://github.com/rohittp0/web-ts-template/issues"
  },
  "homepage": "https://github.com/rohittp0/web-ts-template#readme",
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "node-sass": "^4.14.1",
    "rollup": "^2.23.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "rollup-plugin-uglify": "^6.0.4",
    "typescript": "^3.9.7"
  }
