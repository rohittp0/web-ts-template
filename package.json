{
  "name": "web-ts-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"No test specified. Passing bliendly.\"",
    "build-ts": "rollup -c --environment NODE_ENV:production",
    "build-scss": "rm -rf src/scss/ && node-sass src/scss/ -o dist/css/ --output-style compressed",
    "build": "npm run test && npm run build-ts && npm run build-scss",
    "watch-ts": "rollup -c --watch --environment NODE_ENV:development",
    "watch-scss": "node-sass -w src/scss/ -o dist/css/ --source-map-embed true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rohittp0/web-ts-template.git"
  },
